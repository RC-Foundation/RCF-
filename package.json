{
  "name": "rhizome-community-foundation",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "cross-env NODE_ENV=development vite",
    "build": "cross-env NODE_ENV=production tsc && vite build",
    "build:analyze": "vite build --mode analyze",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview",
    "dev:server": "nodemon --exec ts-node server/index.ts",
    "build:server": "tsc -p server/tsconfig.json",
    "test:server": "jest --config server/jest.config.js",
    "lint:server": "eslint server/**/*.ts",
    "start:prod": "node dist/server/index.js",
    "test": "vitest run",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest run --coverage",
    "typecheck": "tsc --noEmit",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "prepare": "husky install"
  },
  "dependencies": {
    "@sentry/node": "^7.120.0",
    "@sentry/react": "^7.120.0",
    "axios": "^1.7.2",
    "bull": "^4.16.5",
    "cheerio": "^1.0.0",
    "d3": "^7.9.0",
    "express": "^4.21.0",
    "framer-motion": "^11.3.0",
    "ioredis": "^5.6.1",
    "joi": "^17.13.3",
    "leaflet": "^1.9.4",
    "lucide-react": "^0.453.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-intersection-observer": "^9.13.0",
    "react-leaflet": "^4.2.1",
    "react-router-dom": "^6.26.0",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.1",
    "three": "^0.169.0",
    "web-vitals": "^3.5.0",
    "winston": "^3.17.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^19.0.0",
    "@commitlint/config-conventional": "^19.0.0",
    "@eslint/js": "^9.9.1",
    "@testing-library/jest-dom": "^6.4.0",
    "@testing-library/react": "^15.0.0",
    "@testing-library/user-event": "^14.5.0",
    "@types/compression": "^1.8.1",
    "@types/cors": "^2.8.19",
    "@types/d3": "^7.4.3",
    "@types/express": "^4.17.21",
    "@types/express-rate-limit": "^5.1.3",
    "@types/helmet": "^0.0.48",
    "@types/jest": "^29.5.11",
    "@types/leaflet": "^1.9.8",
    "@types/node": "^20.11.0",
    "@types/react": "^18.3.5",
    "@types/react-dom": "^18.3.0",
    "@types/swagger-jsdoc": "^6.0.4",
    "@types/swagger-ui-express": "^4.1.8",
    "@vitejs/plugin-react": "^4.3.1",
    "@vitest/ui": "^1.6.0",
    "autoprefixer": "^10.4.20",
    "cross-env": "^7.0.3",
    "dotenv": "^16.4.0",
    "eslint": "^9.9.1",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-react-hooks": "^5.1.0-rc.0",
    "eslint-plugin-react-refresh": "^0.4.11",
    "globals": "^15.9.0",
    "husky": "^9.0.0",
    "jest": "^29.7.0",
    "jsdom": "^24.0.0",
    "lint-staged": "^15.2.0",
    "msw": "^2.3.0",
    "nock": "^13.4.1",
    "nodemon": "^3.1.0",
    "postcss": "^8.4.47",
    "prettier": "^3.3.0",
    "rollup-plugin-visualizer": "^5.12.0",
    "supertest": "^7.0.0",
    "tailwindcss": "^3.4.0",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.2",
    "typescript": "^5.5.3",
    "typescript-eslint": "^8.3.0",
    "vite": "^5.4.2",
    "vite-plugin-static-copy": "^3.1.0",
    "vitest": "^1.6.0"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{json,css,md}": [
      "prettier --write"
    ]
  }
}
